{
    "name": "Queeg",
    "lowCaseName": "queeg",
    "description": "Quick Resource Access. One click access to MODX Resources from a website to its manager.",
    "author": "Bartholomej",
    "version": "1.2.4",
    "package":{
        "actions": [{
            "id": 1
            ,"controller": "index"
        }],
        "elements": {
            "plugins": [{
                "name": "queeg"
                ,"events": ["OnWebPagePrerender"]
            }]
        },
        "systemSettings": [{
            "key": "published"
            ,"value": 1
            ,"type": "combo-boolean"
            ,"area": "default"
        },
        {
            "key": "timing"
            ,"value": 1
            ,"type": "combo-boolean"
            ,"area": "default"
        },
        {
            "key": "editedon"
            ,"value": 1
            ,"type": "combo-boolean"
            ,"area": "default"
        },
        {
            "key": "editedby"
            ,"value": 1
            ,"type": "combo-boolean"
            ,"area": "default"
        },
        {
            "key": "date_format"
            ,"value": "Y-m-d H:i"
            ,"type": "textfield"
            ,"area": "default"
        },
        {
            "key": "custom_fields"
            ,"value": ""
            ,"type": "textfield"
            ,"area": "default"
        },
        {
            "key": "active"
            ,"value": 1
            ,"type": "combo-boolean"
            ,"area": "default"
        }]
    }
}